{
  "name": "itracksy",
  "productName": "itracksy",
  "version": "1.0.238",
  "description": "A productivity app that tracks time, boosts focus, and provides insights into work habits, helping you manage time effectively and stay productive.",
  "main": ".vite/build/main.js",
  "scripts": {
    "rebuild": "./node_modules/.bin/electron-rebuild; npm rebuild",
    "clear": "rm -rf .vite dist out node_modules",
    "type-check": "tsc --noEmit",
    "dev": "electron-forge start",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "win-make": "npm run clean:out && npm run debug-make",
    "win-build": "cross-env PLATFORM=win32 npm run make",
    "mac-build": "cross-env PLATFORM=darwin npm run make",
    "clean:out": "powershell -Command \"Stop-Process -Name itracksy -Force -ErrorAction SilentlyContinue; Start-Sleep -Seconds 2; Remove-Item -Path out -Force -Recurse -ErrorAction SilentlyContinue\"",
    "publish": "electron-forge publish",
    "prett": "prettier . --write",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest",
    "test:e2e": "playwright test",
    "test:all": "jest && playwright test",
    "screenshots": "./scripts/screenshots-scripts/generate-screenshots.sh",
    "screenshots:verbose": "./scripts/screenshots-scripts/generate-screenshots.sh --verbose",
    "screenshots:quick": "./scripts/screenshots-scripts/generate-screenshots.sh --skip-build",
    "release": "./scripts/release.sh",
    "debug-make": "DEBUG=electron-forge:* npm run make",
    "db:generate": "drizzle-kit generate",
    "db:pull": "drizzle-kit pull",
    "test-permissions": "node scripts/test-permissions.js",
    "db:push": "drizzle-kit push:sqlite --strict",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "tsx src/api/db/migrate.ts",
    "db:apply": "npm run db:generate && npm run db:migrate",
    "db:verify": "tsx src/api/db/migrate.ts --verify-only",
    "db:debug": "DEBUG=drizzle:* npm run db:migrate"
  },
  "author": "HungHoang <hth321@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hunght/itracksy.git"
  },
