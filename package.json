{
  "name": "itracksy",
  "productName": "itracksy",
  "version": "1.0.148",
  "description": "A productivity app that tracks time, boosts focus, and provides insights into work habits, helping you manage time effectively and stay productive.",
  "main": ".vite/build/main.js",
  "scripts": {
    "rebuild": "./node_modules/.bin/electron-rebuild; npm rebuild",
    "clear": "rm -rf .vite dist out node_modules",
    "type-check": "tsc --noEmit",
    "dev": "npm-run-all --parallel type-check:watch dev:backend dev:log",
    "type-check:watch": "tsc --noEmit --watch",
    "dev:frontend": "electron-forge start",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "win-make": "npm run clean:out && npm run debug-make",
    "win-build": "cross-env PLATFORM=win32 npm run make",
    "mac-build": "cross-env PLATFORM=darwin npm run make",
    "clean:out": "powershell -Command \"Stop-Process -Name itracksy -Force -ErrorAction SilentlyContinue; Start-Sleep -Seconds 2; Remove-Item -Path out -Force -Recurse -ErrorAction SilentlyContinue\"",
    "publish": "electron-forge publish",
    "prett": "prettier . --write",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest",
    "test:e2e": "playwright test",
    "test:all": "jest && playwright test",
    "release": "./scripts/release.sh",
    "debug-make": "DEBUG=electron-forge:* npm run make",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "tsx src/api/db/migrate.ts"
  },
  "author": "HungHoang <hth321@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.6.0",
    "@electron-forge/maker-deb": "^7.5.0",
    "@electron-forge/maker-dmg": "^7.6.0",
    "@electron-forge/maker-rpm": "^7.5.0",
    "@electron-forge/maker-squirrel": "^7.5.0",
    "@electron-forge/maker-zip": "^7.5.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.5.0",
    "@electron-forge/plugin-fuses": "^7.5.0",
    "@electron-forge/plugin-vite": "^7.6.0",
    "@electron-forge/publisher-github": "^7.6.0",
    "@electron-forge/shared-types": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "@electron/rebuild": "^3.7.1",
    "@playwright/test": "^1.48.0",
    "@storybook/addon-essentials": "^8.3.5",
