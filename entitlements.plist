<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>com.apple.security.cs.allow-jit</key>
    <true/>
    <key>com.apple.security.cs.allow-dyld-environment-variables</key>
    <true/>
    <key>com.apple.security.cs.disable-library-validation</key>
    <true/>
    <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
    <true/>
    <key>com.apple.security.inherit</key>
    <true/>
    <!-- Critical entitlement for Apple Events and browser URL access -->
    <key>com.apple.security.automation.apple-events</key>
    <true/>
    <!-- Enable AppleScript execution for browser automation -->
    <key>com.apple.security.scripting-targets</key>
    <dict>
      <key>com.apple.Safari</key>
      <array>
        <string>com.apple.event.appleEvent.core</string>
      </array>
      <key>com.google.Chrome</key>
      <array>
        <string>com.apple.event.appleEvent.core</string>
      </array>
      <key>org.mozilla.firefox</key>
      <array>
        <string>com.apple.event.appleEvent.core</string>
      </array>
      <key>com.microsoft.edgemac</key>
      <array>
        <string>com.apple.event.appleEvent.core</string>
      </array>
    </dict>
    <key>com.apple.security.cs.debugger</key>
    <true/>
    <!-- Entitlements for screen recording and accessibility -->
    <key>com.apple.security.device.screen-capture</key>
    <true/>
    <key>com.apple.security.device.audio-input</key>
    <true/>
  </dict>
</plist>
